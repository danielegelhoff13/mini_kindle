void drawPage(const char *pageText)
{
  display.setFont(NULL);   // 5x7 classic font
  display.setTextSize(1); // MUST be 1 for e-ink clarity
  display.setTextColor(GxEPD_BLACK);
  display.setRotation(1);
  display.setFullWindow();
  display.firstPage();

  do {
    display.fillScreen(GxEPD_WHITE);
    display.setCursor(5,5);
    display.print(pageText);
  } while (display.nextPage());

}

void drawStartupImage()
{
  display.setRotation(1);        // adjust if needed
  display.setFullWindow();
  display.firstPage();
  do {
    display.fillScreen(GxEPD_BLACK);
    display.drawBitmap(
      0, 0,
      lotr_bitmap,
      display.width(),
      display.height(),
      GxEPD_WHITE
    );
  } while (display.nextPage());
}


