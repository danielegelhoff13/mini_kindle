void setup() {
  Serial.begin(115200);
  delay(2000);

  display.init(115200, true, 2, false); // USE THIS for Waveshare boards with "clever" reset circuit, 2ms reset pulse
  drawStartupImage();

  delay(9000);

  drawPage(pages[2]);

  delay(9000);

  // drawPage(pages[1]);

  display.powerOff(); 
